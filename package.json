{
  "name": "magento2-module-wordpress-integration-cms",
  "version": "1.0.0",
  "private": true,
  "description": "Magento 2 module for integrating Wordpress pages in Magento CMS pages.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mooore-digital/magento2-module-wordpress-integration-cms.git"
  },
  "bugs": "https://github.com/mooore-digital/magento2-module-wordpress-integration-cms/issues",
  "keywords": [
    "wordpress",
    "magento2",
    "gutenberg",
    "headless"
  ],
  "scripts": {
    "dev": "sass view/frontend/web/css/gutenberg.scss view/frontend/web/css/gutenberg.css --watch ",
    "build": "sass view/frontend/web/css/gutenberg.scss view/frontend/web/css/gutenberg.css -s compressed",
    "less": "lessc view/frontend/web/css/gutenberg.less view/frontend/web/css/gutenberg.css"
  },
  "dependencies": {
    "less": "^2.7.3",
    "sass": "^1.25.0"
  }
}
